<!-- start base/head -->
<meta charset="utf-8">
<title>{{ title | title }} | Tom Hall Archery </title>
{{ m.meta("description",  description ) }}
{{ m.meta("keywords",  keywords ) }}
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="theme-color" content="#ffa700" />

<!-- favicon -->
<!-- generated from https://favicon.io/favicon-generator/ using erica one font -->
<link rel="icon" type="image/png" href="{{ url_for('static', filename='favicon/favicon.ico') }}">
<link rel="apple-touch-icon" sizes="180x180" href="{{ url_for('static', filename='favicon/apple-touch-icon.png') }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ url_for('static', filename='favicon/favicon-32x32.png') }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ url_for('static', filename='favicon/favicon-16x16.png') }}">

<!-- fonts -->
<link rel="preload" href="{{ url_for('static', filename='fonts/Roboto_Slab/RobotoSlab-VariableFont_wght-Latin.woff2') }}" as="font" type="font/woff2" crossorigin>
{{ m.link_style('css/fonts.css')}}

<!-- styles -->
<!-- {{ m.link_style('css/normalize.css')}} -->
{{ m.link_style('css/colors.css')}}
{{ m.link_style('css/styles.css')}}
{{ m.link_style('css/hresize.css')}}
{% if highlight_blocks -%}
{{ m.link_style('css/blocks.css')}}
{%- endif %}

<!-- js -->
<!-- Alpine JS (Modern JQuery replacement for minor animations/ui/reactive parts) -->
<script src="{{ url_for('static', filename='js/alpine.js') }}" defer></script>
<!-- end base/head -->
